        "soma = 0\n",
        "for i in range(5):\n",
        "  numero = float(input(f\"Digite o {i+1}º numero: \"))\n",
        "  soma += numero\n",
        "  media = soma/5\n",
        "\n",
        "print(f\"A soma dos números é: {soma}\")\n",
        "print(f\"A media dos números é: {media}\")"
      ]
    }
  ]
